{
  "extends":"airbnb",
  "env": {
    "es6": true,
    "browser":true,
    "node":true
  },
  "rules":{
    "comma-dangle":"off",
    "no-unused-vars":[
      "warn",
      {
        "vars":"local",
        "args":"none",
      }
    ],
    "no-console":"warn",
    "no-tabs": [
    "error", 
      { 
        "allowIndentationTabs": true 
      }
    ],
    "indent": "off",
    "react/jsx-filename-extension":0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures":{
      "jsx":true
    },
    "sourceType":"module"
  }
}